{
  "name": "trpc_nuxt_starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "preinstall": "node ./scripts/preinstall.js && pnpm add bin-wrapper-china -D -w && china-bin-env",
    "upgrade:nuxt-web": "pnpm -F \"nuxt-web\" upgrade",
    "-------lint---------": "----------------------------",
    "lint:all:eslint": "eslint --fix \"**/src/**/*.{ts,vue,js}\"",
    "lint:all:markdownlint": "pnpm lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "pnpm lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:all:stylelint": "pnpm lint:stylelint \"**/*.{vue,scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:stylelint": "stylelint --fix",
    "typecheck:nuxt-web": "pnpm -F \"nuxt-web\" typecheck",
    "-------test---------": "----------------------------",
    "test": "vitest",
    "test:git": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "-----init-------": "-----------------------------",
    "init:nuxt-web": "pnpm i && pnpm db:generate && pnpm -F \"nuxt-web\" generate",
    "-----apps-------": "-----------------------------",
    "dev:nuxt-web": "pnpm -F \"nuxt-web\" dev",
    "dev:uni:h5": "pnpm -F \"uni-vue3-vite-ts\" dev:h5",
    "dev:uni:mp-weixin": "pnpm -F \"uni-vue3-vite-ts\" dev:mp-weixin",
    "dev": "run-p dev:nuxt-web dev:uni:h5",
    "-----build-------": "-----------------------------",
    "build:nuxt-web": "pnpm -F \"nuxt-web\" build",
    "build:uni:h5": "pnpm -F \"uni-vue3-vite-ts\" build:h5",
    "build:uni:mp-weixin": "pnpm -F \"uni-vue3-vite-ts\" build:mp-weixin",
    "-----db------": "-----------------------------------",
    "db:dev": "pnpm db:up && pnpm db:migrate:dev",
    "db:up": "docker compose -f docker-compose-dev.yml -p trpc_nuxt_starter_dev up -d",
    "db:nuke": "docker compose down --volumes --remove-orphans",
    "db:reset": "pnpm db:nuke && pnpm db:dev",
    "db:migrate:dev": "pnpm -F \"nuxt-web\" db:migrate:dev",
    "db:generate": "pnpm -F \"nuxt-web\" db:generate",
    "db:update": "pnpm -F \"nuxt-web\" db:update",
    "db:migrate:deploy": "pnpm -F \"nuxt-web\" db:migrate:deploy",
    "db:push": "pnpm -F \"nuxt-web\" db:push",
    "db:seed": "pnpm -F \"nuxt-web\" db:seed",
    "db:ui": "pnpm -F \"nuxt-web\" db:ui",
    "zenstack:init": "pnpm -F \"nuxt-web\" zenstack:init",
    "-----docker-------": "-----------------------------",
    "docker-up": "docker-compose up --build --force-recreate -d",
    "docker-build": "docker-compose build --force-rm --no-cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "engines": {
    "node": "^18",
    "pnpm": "^8"
  },
  "prisma": {
    "schema": "apps/nuxt-web/prisma/schema.prisma",
    "seed": "tsx apps/nuxt-web/server/database/seed.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/eslint": "^8.44.0",
    "@types/node": "^20.4.4",
    "@types/prettier": "^2.7.3",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.14",
    "bin-wrapper-china": "^0.1.0",
    "configs": "workspace:*",
    "eslint": "latest",
    "eslint-config-custom": "workspace:*",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.2.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "markdownlint-cli": "^0.37.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "sass": "^1.69.0",
    "stylelint": "^15.10.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-tailwindcss": "^0.0.6",
    "stylelint-order": "^5.0.0",
    "tailwindcss": "^3.3.3",
    "ts-config": "workspace:*",
    "typescript": "^5.1.6",
    "vitest": "^0.34.4"
  }
}
